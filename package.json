{
  "name": "seeker-github-actions",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub Actions for Seeker",
  "scripts": {
    "build": "tsc --build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint-check": "eslint --print-config src/**/*.ts",
    "test": "jest",
    "all": "npm run build && npm run test"
  },
  "author": "",
  "dependencies": {
    "@actions/artifact": "^0.5.2",
    "@actions/core": "^1.4.0",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/node": "^16.3.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vercel/ncc": "^0.29.0",
    "eslint": "^7.31.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.2.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.1.3"
  }
}
